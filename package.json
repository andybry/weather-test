{
  "name": "weather-test",
  "version": "0.0.0",
  "scripts": {
    "lint": "eslint src config webpack.config.js test",
    "clean": "rimraf public lib",
    "compile": "NODE_ENV=production webpack && babel src --out-dir lib",
    "prepublish": "npm run clean && npm run compile",
    "test": "mocha --compilers js:babel-register --recursive --require ./test/setup.js",
    "test:watch": "npm test -- --watch --reporter min",
    "start:dev": "npm run clean && babel-node src/server",
    "start:prod": "node lib/server",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "heroku:deploy": "heroku builds:create -a arb-weather-test",
    "deploy": "npm install && npm prune --production && npm run heroku:deploy"
  },
  "pre-push": [
    "lint",
    "test"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.7.2",
    "chokidar": "^1.4.3",
    "css-loader": "^0.23.1",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.6.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "expect": "^1.16.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jsdom": "^8.2.0",
    "mocha": "^2.4.5",
    "nock": "^8.0.0",
    "node-sass": "^3.7.0",
    "normalize.scss": "^0.1.0",
    "pre-push": "^0.1.1",
    "react-addons-test-utils": "^0.14.8",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.9",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-merge": "^0.8.4"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "express": "^4.13.4",
    "if-env": "^1.0.0",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.13.0",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.1",
    "redux": "^3.3.1"
  }
}
